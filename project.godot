[application]

config/icon = "res://icon.png"
config/name = "Cubiventure"
run/main_scene = "res://scenes/game.tscn"
target_fps = 30
use_shared_user_dir = false

[autoload]

globals = "*res://scripts/globals.gd"

[display]

window/size/borderless = false
window/vsync/use_vsync = true

[importer_defaults]

texture = {
"flags/filter": true,
"flags/mipmaps": true
}

[physics]

common/physics_fps = 60

[rasterizer]

fp16_framebuffer = false
max_shadow_buffer_size = 1024
shadow_filter = 0
trilinear_mipmap_filter = false
use_fragment_lighting = false

[rasterizer.Android]

use_fragment_lighting = true

[rendering]

threads/thread_model = 2

